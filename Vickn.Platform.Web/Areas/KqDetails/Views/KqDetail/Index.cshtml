@using System.Web.Optimization
@using System.Web.UI.WebControls
@using System.Web.UI.WebControls.Expressions
@using Abp.Authorization
@using Abp.Web.Mvc.Extensions
@using Vickn.Platform.Attendances.KqDetails.Authorization



<div class="row animated fadeInRight">
    <div class="ibox-content">
        <div class="form-inline">
            <div class="form-group">
                <label>@L("FilterText")</label>
                <input type="text" placeholder="@L("User.Name")" class="form-control" id="filterText" style="width: 150px;" />
                
                <label>@L("StartTime")</label>
                <input type="text" Id="time1" class="form-control" style="width: 150px;" />
                <label>@L("EndTime")</label>
                <input type="text" Id="time2" class="form-control" style="width: 150px;" />

                <button type="button" class="btn btn-default" id="research"><span class="glyphicon glyphicon-refresh"></span> @L("ReSearch")</button>
                <button type="submit" class="btn btn-success"  ><span class="glyphicon glyphicon-search"></span> @L("Search")</button>




            </div>
        </div>
    </div>

    <div class="ibox-content">
        <table class="table table-bordered table-hover dataTable">
            <thead>
                <tr>
                    <th width="25"><input type="checkbox" class="checkall"></th>
                    <th>@L("User.Name")</th>
                    <th>@L("QDTime")</th>
                    <th>@L("QDType")</th>
                    <th>@L("QDPosition")</th>

                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>



@section styles{

    <link href="~/Content/LayDate/theme/default/laydate.css" rel="stylesheet" />
}

@section scripts
{


    <script src="~/Areas/KqDetails/Views/KqDetail/Index_js.js"></script>
    <script src="~/Content/LayDate/laydate.js"></script>

}