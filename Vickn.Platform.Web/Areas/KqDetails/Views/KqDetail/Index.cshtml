@using System.Web.Optimization
@using System.Web.UI.WebControls
@using System.Web.UI.WebControls.Expressions
@using Abp.Authorization
@using Abp.Web.Mvc.Extensions
@using Vickn.Platform.Attendances.Statisticls.Authorization;



<div class="row animated fadeInRight">
    <div class="ibox-content">
        <div class="form-inline">
            <div class="form-group">
                <label>@L("FilterText")</label>
                <input type="text" placeholder="@L("User.Name")" class="form-control" id="filterText" style="width: 250px;" />

                <button type="button" class="btn btn-default" id="research"><span class="glyphicon glyphicon-refresh"></span> @L("ReSearch")</button>
                <button type="button" class="btn btn-success" id="search" name=""><span class="glyphicon glyphicon-search"></span> @L("Search")</button>

              


            </div>
        </div>
    </div>

    <div class="ibox-content">
        <table class="table table-bordered table-hover dataTable">
            <thead>
                <tr>
                    <th width="25"><input type="checkbox" class="checkall"></th>
                    <th>@L("User.Name")</th>
                    <th>@L("QDTime")</th>
                    <th>@L("QDType")</th>
                    <th>@L("QDPosition")</th>
                   
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>

@section styles{

}

@section scripts{
    @Html.IncludeScript("~/Areas/KqShifts/Views/KqShift/index_js.js")
}

