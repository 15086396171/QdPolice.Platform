@using System.Web.Optimization
@using System.Web.UI.WebControls
@using System.Web.UI.WebControls.Expressions
@using Abp.Authorization
@using Abp.Web.Mvc.Extensions
@using Vickn.Platform.Attendances.KqDetails.Authorization



<div class="row animated fadeInRight">
    <div class="ibox-content">
        <div class="form-inline">
            <div class="form-group">
                <label>@L("User.Name")</label>
                <input type="text" placeholder="@L("User.Name")" class="form-control" id="UserName" style="width: 150px;" />

                <label>@L("StartTime")</label>
                <input type="text" Id="StartTime" value="@ViewBag.StartTime" onclick="laydate()" class="form-control" style="width: 150px;" />
                <label>@L("EndTime")</label>
                <input type="text" Id="EndTime" onclick="laydate()" value="@ViewBag.EndTime" class="form-control" style="width: 150px;" />
                <label>@L("QDType")</label>
                <select class="form-control valid" id="IsNFC" name="IsNFC" aria-invalid="false">

                    <option value="所有">所有</option>
                    <option value="0">微信打卡</option>
                    <option value="1">NFC打卡</option>
                    <option value="2">门禁刷卡</option>


                </select>
                <label>@L("KqShift.Name")</label>

                <select class="form-control valid" id="KqShiftName" name="KqShiftName" aria-invalid="false">
                    @foreach (var item in ViewBag.KqShiftName)
        {
            <option value="@item.ShiftName">@item.ShiftName</option>

}
                </select>

                <button type="button" class="btn btn-default" id="research" ><span class="glyphicon glyphicon-refresh"></span> @L("ReSearch")</button>
                <button type="button" class="btn btn-success" id="search"><span class="glyphicon glyphicon-search"></span> @L("Search")</button>




            </div>
        </div>
    </div>

    <div class="ibox-content">
        <table class="table table-bordered table-hover dataTable">
            <thead>
                <tr>

                    <th>@L("User.Name")</th>
                    <th>@L("GroupName")</th>
                    <th>@L("KqShift.Name")</th>
                    <th>@L("QDTime")</th>
                    <th>@L("QDType")</th>
                    <th>@L("OutgoingCause")</th>
                    <th>@L("QDPosition")</th>

                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>



@section styles{


}

@section scripts
{


    <script src="~/Areas/KqDetails/Views/KqDetail/Index_js.js"></script>


}