@using Abp.Application.Services.Dto
@using Abp.Web.Mvc.Extensions
@using Vickn.Platform.DataDictionaries
@model List<Vickn.Platform.PbManagement.PositionPbTimes.Dtos.PositionPbUserTimeListDto>



<div class="row animated fadeInRight">

  
    <div class="ibox-content">
       
        <table class="table table-bordered table-hover dataTable">
            <thead>
            <tr>
                <th>@L("Close")</th>
                <th>@L("User.Name")</th>
                <th>@L("PositionName")</th>
                <th>@L("ChangeWorkTime")</th>

            </tr>
            </thead>
            <tbody id="tbody">
              

            @foreach (var str in Model) {
                <tr>
                    <td><input type="radio"  name="check" remark="@str.UserName" value="@str.PositionPbTimeId"></td>
                    <td> @str.UserName</td>
                    <td> @str.PositionName</td>
                    <td> @str.WorkTime</td>
                </tr>
            }

            </tbody>
        </table>
    </div>

</div>


