@using Abp.Web.Mvc.Extensions
<div class="row">
<div class="col-sm-3">
  <div class="ibox float-e-margins">
    <div class="ibox-content">
      <div class="file-manager">
        <h5>显示：</h5>
        <a href="javascript:;" id="all" class="file-control active">所有</a>
        <a href="javascript:;" class="file-control">录音</a>
        <a href="javascript:;" class="file-control">视频</a>
        <a href="javascript:;" class="file-control">图片</a>
        <div class="hr-line-dashed"></div>
        <button class="btn btn-info btn-block" onclick="window.history.back();">返回</button>
        <div class="hr-line-dashed"></div>
        <h5>文件夹</h5>
        <ul class="folder-list" style="padding: 0">
          <li>
            <a href="javascript:;"><i class="fa fa-folder"></i> 录音</a>
          </li>
          <li>
            <a href="javascript:;"><i class="fa fa-folder"></i> 视频</a>
          </li>
          <li>
            <a href="javascript:;"><i class="fa fa-folder"></i> 图片</a>
          </li>
        </ul>
        <h5 class="tag-title">标签</h5>
        <ul class="tag-list" style="padding: 0" id="tag">
          @*<li>
            <a href="javascript:;">爱人</a>
          </li>
          <li>
            <a href="javascript:;">工作</a>
          </li>
          <li>
