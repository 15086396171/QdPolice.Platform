@using System.Web.Optimization
@using System.Web.UI.WebControls
@using System.Web.UI.WebControls.Expressions
@using Abp.Authorization
@using Abp.Web.Mvc.Extensions
@using Vickn.Platform.Attendances.Statisticls.Authorization;



<div class="row animated fadeInRight">
    <div class="ibox-content">
        <div class="form-inline">
            <div class="form-group">
                <label>@L("FilterText")</label>
                <input type="text" placeholder="@L("FilterText")" class="form-control" id="UserName" value="@ViewBag.UserName" style="width: 250px;" />
                <label>@L("StartTime")</label>
                <input type="text" Id="StartTime" onclick="laydate()" value="@ViewBag.StartTime" class="form-control" style="width: 150px;" />
                <label>@L("EndTime")</label>
                <input type="text" Id="EndTime" onclick="laydate()"  value="@ViewBag.EndTime" class="form-control" style="width: 150px;" />

                <button type="button" class="btn btn-default" id="research"><span class="glyphicon glyphicon-refresh"></span> @L("ReSearch")</button>
                <button type="button" class="btn btn-success" id="search" name=""><span class="glyphicon glyphicon-search"></span> @L("Search")</button>

                <a href="javascript:;" class="btn btn-primary" id="Btnkqmonth">
                    <span class="glyphicon glyphicon-plus"></span> @L("KqMonth")
                </a>

                <a href="javascript:;" class="btn btn-danger" id="Btnkqyear"><span class="glyphicon glyphicon-trash"></span> @L("KqYear")</a>


            </div>
        </div>
    </div>

    <div class="ibox-content">
        <table class="table table-bordered table-hover dataTable">
            <thead>
                <tr>

                    <th>@L("User.Name")</th>
                    <th>@L("GroupName")</th>
                    <th>@L("Normal.days")</th>
                    <th>@L("Late.days")</th>
                    <th>@L("LeaveEarly.days")</th>
                    <th>@L("Absenteeism.days")</th>
                    <th>@L("Operational")</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>


@section styles{


}

@section scripts
{


    <script src="~/Areas/KqStatistics/Views/KqStatistic/Index_js.js"></script>


}
