@using Abp.Web.Mvc.Extensions
@using Vickn.Platform.Users.Authorization
@model Vickn.PlatfForm.Utils.Pager.PagerResult<Vickn.Platform.Auditing.Dto.AuditLogListDto, Vickn.Platform.Auditing.Dto.GetAuditLogsInput>

@{
    ViewBag.Menu = L("SystemManage");
    ViewBag.SubMenu = L("AuditLog");
    ViewBag.createBtn = L("CreateAuditLog");
    ViewBag.editBtn = L("EditAuditLog");

    ViewBag.ActiveMenu = "AuditLogs"; //The menu item will be active for this page.

}
<div class="ibox-content">
    <div class="form-inline">
        <div class="form-group">
            <input class="form-control layer-date laydate-icon" placeholder="请输入开始时间" id="startDate" onclick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})">
            <input class="form-control layer-date laydate-icon" placeholder="请输入结束时间" id="endDate" onclick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})">
            <button type="button" class="btn btn-success" id="search" name=""><span class="glyphicon glyphicon-search"></span> @L("Search")</button>
            <button type="button" class="btn btn-default" id="research"><span class="glyphicon glyphicon-refresh"></span> @L("ReSearch")</button>
        </div>
    </div>
</div>
<div class="ibox-content">
    <table class="table table-border table-bordered table-hover table-bg dataTable ">
        <thead>
            <tr class="text-c">
                <th width="25"></th>
                <th width="25">状态</th>
                <th>时间</th>
                <th>用户名</th>
                <th>服务</th>
                <th>操作</th>
                <th>持续时间</th>
                <th>IP地址</th>
                <th>客户端</th>
                <th>浏览器</th>
            </tr>
        </thead>
    </table>
</div>
@section scripts{
    @Html.IncludeScript("~/Views/AuditLogs/Index.js")
}
